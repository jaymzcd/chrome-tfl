{
  "name": "Tubeme",
  "version": "0.2",
  "description": "Grabs the current TFL/London tube data, customizable update time.",
   "icons": { "128": "tflicon.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_badge" : "1",
    "default_title" : "So how bad is it?",
    "popup" : "popup.html",
    "background_page" : "background.html"
  },
  "permissions": [
    "http://api.tubeupdates.com/",
    "http://ajax.googleapis.com/"
  ]
}